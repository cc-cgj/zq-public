.title {
    font-size: 16px;
    margin-top: 10px;
    position: relative;
    padding-left: 20px;
    padding-right: 10px;
}

.title:before {
    content: '';
    width: 3px;
    height: 18px;
    border-radius: 1px;
    background-color: #FC490F;

    position: absolute;
    top: 2px;
    left: 10px;
}
